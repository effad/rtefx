<REDConfig>
	<BracketMatcher id="Test2">
		<RuleSet id="rules">
			<BeforeLeft>false</BeforeLeft>
			<AfterLeft>false</AfterLeft>
			<BeforeRight>false</BeforeRight>
			<AfterRight>false</AfterRight>
			<DoubleClickSelect>false</DoubleClickSelect>
		</RuleSet>
		<StyleSet id="styles">
		</StyleSet>
		<Pattern RuleSet="rules" StyleSet="styles">
			<Left>|</Left>
			<Right>-</Right>
		</Pattern>
		<MaxLines>99</MaxLines>
		<MaxChars>499</MaxChars>
	</BracketMatcher>
</REDConfig>
