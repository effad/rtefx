<?xml version="1.0"?>
<!--
Test bracket matcher definition. This tries defect pattern
rli@chello.at
-->
<REDConfig>
	<BracketMatcher id="Tilt2">
		<StyleSet id="styles"/>
		<RuleSet id="rules"/>
		<Pattern StyleSet="styles" RuleSet="rules">
			<Left>{</Left>
		</Pattern>
		<Pattern>
			<Left>(</Left>
			<Right>)</Right>
		</Pattern>
	</BracketMatcher>
</REDConfig>
